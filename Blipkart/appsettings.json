{
    "ConnectionStrings": {
        "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=Ekart;Trusted_Connection=True;MultipleActiveResultSets=true"
    },
    "Session": {
        "CookieName": ".Blipkart.Session",
        "TimeOut": 20
    },
    "Authentication": {
        "Scheme": "BlipkartCookieMiddlewareInstance",
        "LoginUrl": "/Account/Login",
        "AccessDeniedPath": "/Account/Forbidden"
    },
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
          "Default": "Debug",
          "System": "Information",
          "Microsoft": "Information"
        }
    }
}
